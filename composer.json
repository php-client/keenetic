{
  "name": "php-client/keenetic",
  "description": "PHP Client for Keenetic Router API",
  "keywords": ["keenetic", "api", "client", "saloon"],
  "type": "library",
  "require": {
    "php": "^8.3",
    "phpclient/support": "dev-master",
    "saloonphp/saloon": "^3.9"
  },
  "require-dev": {
    "pestphp/pest": "^2.34",
    "symfony/var-dumper": "^7.1",
    "vlucas/phpdotenv": "^5.6"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../support"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "PhpClient\\Keenetic\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
